{
  "compilerOptions": {
  "noEmit": true,
  "target": "ESNext",
  "lib": ["ESNext", "DOM"],
  "allowSyntheticDefaultImports": true,
  "jsx": "react-jsx",
  "moduleResolution": "nodenext",
  "module": "NodeNext",
  "types": ["node"],
    "baseUrl": "src",
    "paths": {
      "@/*": ["*"],
      "@/components/*": ["components/*"],
      "@/lib/*": ["lib/*"]
    },
    "allowImportingTsExtensions": true,
  }
}
